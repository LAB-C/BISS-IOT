{
  "globals": {
    "rootRequire": true,
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": ["error", "always-multiline"],
    "global-require": 0,
    "func-names": 0,
    "function-paren-newline": 0,
    "semi": 0,
    "prefer-arrow-callback": 0,
    "eqeqeq": 0,
    "wrap-iife": 0,
    "no-unused-expressions": 0,
    "no-console": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "no-multi-assign": 0,
    "import/no-dynamic-require": 0,
    "object-curly-newline": 0,
    "no-use-before-define": ["error", { "functions": false }],
  }
}
